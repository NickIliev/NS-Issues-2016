<Page loaded="loaded">
  <GridLayout>
      <Label text="{{ price }}"  horizontalAlignment="left" verticalAlignment="center"/>
      <ListView items="{{ modelList }}" >
        <ListView.itemTemplate>
          <Label text="{{ model }}"  horizontalAlignment="left" verticalAlignment="center" tap="display"  />  
        </ListView.itemTemplate>
      </ListView>    
  </GridLayout>
</Page>